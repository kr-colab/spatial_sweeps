#!/bin/bash
#SBATCH --partition=kern,kerngpu
#SBATCH --job-name=paR
#SBATCH --output=logs/%j\.out
#SBATCH --error=logs/%j\.err
#SBATCH --time=00:10:00
#SBATCH --account=kernlab
#SBATCH --mem=2000MB

filepath=$1
sim=$2
scoef=$3
Nw=$4
tick=$5
outpath=$6

echo "$filepath" "$sim" "$scoef" "$Nw" "$tick" $( Rscript power_analysis.R --infile $filepath ) >> $outpath

